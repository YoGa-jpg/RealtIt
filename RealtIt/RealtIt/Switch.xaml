<UserControl x:Class="RealtIt.Switch"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RealtIt"
             mc:Ignorable="d" 
             d:DesignHeight="450" Width="966.621">
    <Viewbox Margin="0,0,2,1">
        <Grid HorizontalAlignment="Left" Height="31" Margin="0" VerticalAlignment="Top" Width="121" MouseLeftButtonDown="SwitchChange">
            <Rectangle Fill="#332E2E" StrokeThickness="0"/>
            <Rectangle x:Name="SwitchDot" Margin="0,0,83,0" Fill="#4E703E" StrokeThickness="0"/>
        </Grid>
    </Viewbox>
